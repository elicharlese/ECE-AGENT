{
  "name": "desktop",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/desktop/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc",
        "cwd": "apps/desktop"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "apps/desktop"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dist",
        "cwd": "apps/desktop"
      }
    }
  }
}
